{
  "name": "webpack-react-typescript-template",
  "version": "1.0.0",
  "description": "Template for create react app using webpack and typescript",
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "react-router",
    "i18next",
    "eslint",
    "prettier",
    "husky",
    "commitlint"
  ],
  "author": "",
  "homepage": "",
  "scripts": {
    "start": "webpack serve --progress --mode development",
    "clean": "rimraf -rf ./build",
    "build": "npm run clean && webpack --progress --mode production",
    "lint": "eslint src --ext .js,.ts,.tsx --ignore-pattern '!.*.js' --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.1",
    "@commitlint/config-conventional": "^17.1.0",
    "@sanlamcx/react-ui-next-paper": "^15.44.84",
    "@sanlamcx/react-ui-next-provider": "^15.44.84",
    "@sanlamcx/react-ui-next-typography": "^15.44.84",
    "@svgr/webpack": "^6.3.1",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^7.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.0",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "i18next": "^21.9.2",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-resources-to-backend": "^1.0.0",
    "@tanstack/react-query": "^4.13.5",
    "axios": "^1.2.0",
    "flagged": "^2.0.6",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.18.6",
    "react-router-dom": "^6.4.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --cache --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ]
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0"
  }
}
